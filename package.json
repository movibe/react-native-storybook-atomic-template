  "sideEffects": false,
  "scripts": {
    "bootstrap": "yarn --cwd example && yarn",
    "build": "npm run build:clean && bob build && node ./scripts/generate-mappings.js",
    "build:clean": "rimraf dist && rimraf lib",
    "release": "release-it",
    "klank": "klank storybook-static/index.html",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "commit": "git-cz",
    "lint:fix": "npm run lint --fix",
    "g": "scaffdog generate",
    "reg-suit": "reg-suit run",
    "storybook": "start-storybook -p 9001 -c .storybook -s src/assets ",
    "prebuild-storybook": "npm run test:generate-output",
    "build-storybook": "build-storybook -c .storybook -s .storybook/assets,./assets -o public_storybook",
    "storybook:screenshot": "rm -rf ./__screenshots__ && yarn storycap http://localhost:9001 --viewport '414x736' --serverTimeout '200000'",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test": "jest ",
    "testdev": "jest --watch --coverage ",
    "testview": "npx live-server reports/lcov-report ",
    "tsc": "rimraf dist && tsc",
    "format": "prettier-eslint --write $PWD/\"src/**/*.{ts, tsx}\"",
    "predeploy": "npm run build-storybook",
    "deploy": "chromatic --project-token vsoyvnqnzl"
  },
